{
  "env": {
    "es6": true,
    "commonjs": true,
    "node": true
  },
  "extends": ["airbnb-typescript/base", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "project": "./tsconfig.json"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "import/extensions": ["off", "never"],
    "no-console": "off",
    "no-param-reassign": "off",
    "consistent-return": "off"
  }
}
